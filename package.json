{
  "name": "terrain-gen",
  "version": "1.0.0",
  "description": "Terrain Generation for DnD World Building",
  "main": "index.js",
  "scripts": {
    "start": "npx tsc && node dist/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rileylum/terrain-gen.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rileylum/terrain-gen/issues"
  },
  "homepage": "https://github.com/rileylum/terrain-gen#readme",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/sharp": "^0.31.1",
    "sharp": "^0.34.2"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "jest --findRelatedTests --passWithNoTests"
    ]
  }
}
